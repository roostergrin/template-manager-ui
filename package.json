    "jsdom": "^26.1.0",
    "sass-embedded": "^1.77.8",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.1",
    "vitest": "^3.2.4"
  },
  "optionalDependencies": {
    "sass-embedded-darwin-x64": "^1.90.0"
  }
